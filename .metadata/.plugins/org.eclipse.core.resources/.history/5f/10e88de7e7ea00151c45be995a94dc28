class City(object):
    '''
    classdocs
    '''
    


    def __init__(self, name):
        '''
        Constructor
        '''
        
        self.name
        self.neighbors = []
        return
    
        
    def addNeighbor(self, num, dist):
        self.neighbors.append((num,dist )
                              return
                          



        