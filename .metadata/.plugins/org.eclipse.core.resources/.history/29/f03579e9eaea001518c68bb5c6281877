def makeMap(fileName):
    cityMap =[]
    
    mapFile = open(fileName)
    line = mapFile.readline()
    
    while line != "":
        print(mapFile.readline())
        line = mapFile.readline()
        
    
    
    mapFile.close()
    return cityMap
