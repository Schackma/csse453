def makeMap(fileName):
    cityMap =[]
    
    mapFile = open(fileName)
    line = mapFile.readline()
    
    while line != "":
        [city1,city2,dist] = line.split(" ")
        
        
        line = mapFile.readline()
        
    mapFile.close()
    return cityMap
